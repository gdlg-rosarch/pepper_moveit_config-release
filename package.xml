  </description>
  <author email="natalia.lyubova@gmail.com">Natalia Lyubova</author>
  <maintainer email="natalia.lyubova@gmail.com">Natalia Lyubova</maintainer>

  <license>BSD</license>

  <url type="bugtracker">https://github.com/nlyubova/pepper_moveit_config/issues</url>
  <url type="repository">https://github.com/nlyubova/pepper_moveit_config</url>

  <build_depend>pepper_description</build_depend>
  <run_depend>pepper_description</run_depend>
    
  <run_depend>moveit_ros_move_group</run_depend>
